$main_i = 0;
foreach ($results as $row):


    //finds the continent of a country and places it in its continent array.        
    if (Mage::helper('geoipultimatelock')->getcontinent($row["cn"]) == 1) {
        $africa[] = $row["cn"];
        $africacc[] = $row["cc"];
