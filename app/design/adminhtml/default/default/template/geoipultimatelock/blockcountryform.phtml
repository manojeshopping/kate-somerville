        $oceania[] = $row["cn"];
        $oceaniacc[] = $row["cc"];
    } else if (Mage::helper('geoipultimatelock')->getcontinent($row["cn"]) == 6) {
        $samerica[] = $row["cn"];
        $samericacc[] = $row["cc"];
    } else if (Mage::helper('geoipultimatelock')->getcontinent($row["cn"]) == 7) {
        $others[] = $row["cn"];
        $otherscc[] = $row["cc"];
    }


endforeach;
