        $europe[] = $row["cn"];
        $europecc[] = $row["cc"];
    } else if (Mage::helper('geoipultimatelock')->getcontinent($row["cn"]) == 4) {
        $namerica[] = $row["cn"];
        $namericacc[] = $row["cc"];
