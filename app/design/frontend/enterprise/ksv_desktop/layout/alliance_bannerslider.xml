<layout version="0.1.0">

    <default>
        <reference name="head">
            <block type="core/text" name="appointment.google.cdn.jquery.ui.css">
                <action method="setText">
                    <text><![CDATA[<script src="//malsup.github.io/jquery.cycle2.js"></script>]]></text>
                </action>
            </block>
        </reference>
    </default>

    <cms_index_index>
        <reference name="content">
            <block type="alliance_bannerslider/slider" template="alliance_bannerslider/slider.phtml" name="alliance_bannerslider_slider">
                <action method="setWidth"><pixels>691</pixels></action>
                <action method="setHeight"><pixels>470</pixels></action>
                <action method="setDelay"><milliseconds>5000</milliseconds></action>
                <action method="loadBannerCollection"><slider_code>homepage</slider_code></action>
            </block>
			<block type="core/template" template="cms/widgets.phtml" name="add_widgets" />
        </reference>
    </cms_index_index>

</layout>